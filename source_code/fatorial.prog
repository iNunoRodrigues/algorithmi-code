    BEGIN MAIN_PROGRAM_NAME
        READ INTEGER x 
        DEFINE INTEGER f SET fact(x)
        WRITE x SUM "! = " SUM f
    END MAIN_PROGRAM_NAME

FUNCTION INTEGER fact ROUND_OPEN INTEGER n ROUND_CLOSE
        IF n GREATHER_THAN 2
RETURN n MULT fact(n - 1)
        ELSE
RETURN n
        END IF

    END fact